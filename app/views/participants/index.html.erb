<div class="row">
<% @users.each do |user| %>
	<div class="col-lg-12">
		<div class="col-lg-2">
			<%= image_tag "parti.jpg" %>
		</div>
		<div class="col-lg-4">
			<% user.attributes.each do |key,value| %>
				<% next if key.eql?("_id") %>
				<h4><%= key %>  <small><%= value %></small></h4>
			<% end %>
		</div>
	</div>
	<br>
<% end %>
</div>